# Production Version
sudo -i flock -xn /tmp/ASB.lck env GIT_SSL_NO_VERIFY=true ansible-pull --url=http://gerrit.sjc.aristanetworks.com/ardc-config --directory=/root/.ansible/pull ASB/ASB.yml --inventory=localhost,

# Staging Version
sudo -i flock -xn /tmp/ASB.lck env GIT_SSL_NO_VERIFY=true ansible-pull --url=https://gitlab.aristanetworks.com/ren/ASB.git --directory=/root/.ansible/pull ASB/ASB.yml --inventory=localhost, --extra-vars "BYPASS=true"
